{
	"name": "@monejs/vue-admin-layout",
	"version": "0.0.7",
	"private": false,
	"description": "A Layout Component of Admin Template, Support Vue2 and Vue3",
	"author": "Mone <2361954836@qq.com>",
	"type": "module",
	"main": "./dist/index.umd.js",
	"module": "./dist/index.es.js",
	"types": "./dist/index.d.ts",
	"exports": {
		".": {
			"import": "./dist/index.es.js",
			"require": "./dist/index.umd.js",
			"types": "./dist/index.d.ts"
		},
		"./style.css": "./dist/style.css"
	},
	"files": [
		"dist"
	],
	"keywords": [
		"admin",
		"layout",
		"admin-layout",
		"vue2",
		"vue3"
	],
	"scripts": {
		"dev": "cross-env VITE_IS_LIB=true vite",
		"build": "pnpm run typecheck && cross-env VITE_IS_LIB=false vite build",
		"build:lib": "pnpm run typecheck && cross-env VITE_IS_LIB=true vite build",
		"typecheck": "vue-tsc --noEmit",
		"preview": "vite preview",
		"public": "pnpm publish --access public"
	},
	"peerDependencies": {
		"@vue/composition-api": "^1.0.0-rc.1",
		"vue": "^2.0.0 || >=3.0.0"
	},
	"peerDependenciesMeta": {
		"@vue/composition-api": {
			"optional": true
		}
	},
	"dependencies": {
		"@monejs/vue-admin-layout": "file:dist",
		"vue": "^3.2.45",
		"vue-demi": "^0.13.11"
	},
	"devDependencies": {
		"@types/node": "^18.14.0",
		"@vitejs/plugin-vue": "^4.0.0",
		"cross-env": "^7.0.3",
		"less": "^4.1.3",
		"typescript": "^4.9.3",
		"unocss": "^0.50.6",
		"unplugin-vue-define-options": "^1.2.1",
		"vite": "^4.1.0",
		"vite-plugin-dts": "^2.0.2",
		"vue-tsc": "^1.0.24"
	},
	"publishConfig": {
		"access": "public",
		"registry": "https://registry.npmjs.org/"
	}
}
